#include <stdio.h>
#include <stdlib.h>

int main()
{
    float a,b;

    printf("Digite dois numeros reais:\n");
        scanf("%f",&a);
        scanf("%f",&b);

    printf("\nnumero real 1: %.2f",a);
    printf("\nnumero real 2: %.2f",b);

    printf("\nnumero inteiro 1: %.2f",round(a));
    printf("\nnumero inteiro 2: %.2f",round(b));

    return 0;
}
